{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-console": false,
    "no-empty": false,
    "forin": false,
    "variable-name": false,
    "prefer-const": false,
    "no-shadowed-variable": false,
    "triple-equals": false,
    "no-empty-interface": false
  }
}
